{
  "id": "py-io-021",
  "title": "日志筛选官",
  "lang": "python",
  "gameType": "io",
  "difficulty": 3,
  "story": "筛选灯塔日志中的告警行（包含 [WARN] 或 [ERROR]）。",
  "goals": ["读取多行，输出含关键字的行"],
  "starter": { "blockly": "<xml/>", "code": "# 连续读取直到空行，输出含 [WARN] 或 [ERROR] 的行\n" },
  "grader": {
    "mode": "io",
    "io": {
      "cases": [{ "in": "[INFO] ok\\n[WARN] low\\n\\n", "out": "[WARN] low\\n" }],
      "match": "regex",
      "pattern": "(?s).*"
    }
  },
  "rewards": { "xp": 28, "coins": 10, "badges": ["灯塔信使"] },
  "hints": ["in 子串判断", "循环读取，空行结束"]
}
